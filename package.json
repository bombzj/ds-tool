  "scripts": {
    "pkg": "pkg . --output dist/ZestImage.exe --compress GZip"
  },
  "dependencies": {
    "axios": "0.27.2",
    "bwip-js": "^4.0.0",
    "jimp": "^0.22.10",
    "pdf-lib": "^1.17.1",
    "progress": "^2.0.3",
    "rcedit": "^3.1.0",
    "sharp": "^0.32.4",
    "xlsx": "^0.18.5"
  }
}
