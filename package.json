{
  "name": "zestimage",
  "version": "0.5",
  "description": "xls split png",
  "copyright": "Copyright Â©Zesty 2023",
  "main": "splitImage.js",
  "bin": "splitImage.js",
  "pkg": {
    "targets": [
      "node18-win-x64"
    ],
    "assets": [
      "node_modules/sharp/build/Release/**/*",
      "node_modules/@jimp/plugin-print/fonts/**/*"
